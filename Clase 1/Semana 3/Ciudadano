public class Ciudadano
{
    String provincia;
    long dni;
    
    public void setProvincia(String provincia){
        this.provincia = provincia;
    }
    
    public String getProvincia(){
        return this.provincia;
    }
    
    public void setDni(long dni){
        this.dni = dni;
    }
    
    public long getDni(){
        return this.dni;
    }
}
